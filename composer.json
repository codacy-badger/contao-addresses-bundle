{
  "name": "w3scout/contao-addresses-bundle",
  "description":"A Contao module to add more then one address to a member",
  "keywords":["contao", "addresses", "addressbook", "members"],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Darko Selesi",
      "homepage": "https://github.com/w3scout/contao-addresses-bundle",
      "role":"Developer"
    }
  ],
  "support":{
    "issues":"https://github.com/w3scout/contao-addresses-bundle/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/w3scout/contao-addresses-bundle"
  },
  "require": {
    "php": ">=5.6.0",
    "contao/core-bundle": "^4.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "W3Scout\\AddressesBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "W3Scout\\AddressesBundle\\ContaoManager\\Plugin"
  }
}